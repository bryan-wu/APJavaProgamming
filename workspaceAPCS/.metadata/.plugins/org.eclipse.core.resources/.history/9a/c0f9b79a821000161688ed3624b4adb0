public class Tile
{
    private int row;

    private int col;

    private Color c;


    /**
     * @param r
     *            row
     * @param c
     *            col
     * @param players
     *            number of
     */
    public Tile( int r, int c )
    {
        row = r;
        col = c;
    }

}
